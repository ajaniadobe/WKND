{
  "definitions": [
    {
      "title": "API Lookup",
      "id": "apilookup",
      "plugins": {
        "xwalk": {
          "page": {
            "resourceType": "core/fd/components/form/panelcontainer/v1/panelcontainer",
            "template": {
              "jcr:title": "DUNS Lookup",
              "fieldType": "panel",
              "fd:viewType": "dunslookup",
              "text": {
                "value": "Text related to the API lookup here.",
                "sling:resourceType": "core/fd/components/form/text/v1/text",
                "fieldType": "plain-text",
                "textIsRich": true
              },
              "link": {
                "enum": [
                  "dummy-link-1"
                ],
                "type": "string[]",
                "fd:viewType": "toggleable-link",
                "title": "I agree to the terms & conditions",
                "enumNames": [
                  "Privacy policy."
                ],
                "sling:resourceType": "core/fd/components/form/checkboxgroup/v1/checkboxgroup",
                "fieldType": "checkbox-group",
                "fd:rules": {
                  "jcr:primaryType": "nt:unstructured",
                  "validationStatus": "valid"
                },
                "fd:events": {
                  "jcr:primaryType": "nt:unstructured",
                  "change": [
                    "if(length($field.$value) == length($field.$enum), dispatchEvent($parent.approvalcheckbox, 'custom:setProperty', {enabled : true()}), {})"
                  ]
                }
              },
              "approvalcheckbox": {
                "name": "approvalcheckbox",
                "jcr:title": "I agree to the terms & conditions.",
                "sling:resourceType": "core/fd/components/form/checkbox/v1/checkbox",
                "fieldType": "checkbox",
                "required": true,
                "type": "string",
                "enum": [
                  "true"
                ]
              },
              "companyname": {
                "resourceType": "core/fd/components/form/textinput/v1/textinput",
                "template": {
                  "jcr:title": "Company Name",
                  "fieldType": "text-input",
                  "enabled": true,
                  "visible": true
                }
              },
              "dunsnumber": {
                "resourceType": "core/fd/components/form/textinput/v1/textinput",
                "template": {
                  "jcr:title": "DUNS Number",
                  "fieldType": "text-input",
                  "enabled": false,
                  "visible": false
                }
              }
            }
          }
        }
      }
    }
  ],
  "models": [
    {
      "id": "apilookup",
      "fields": [
        {
          "component": "container",
          "name": "basic",
          "label": "Basic",
          "collapsible": false,
          "fields": [
            {
              "...": "../../models/form-common/_basic-input-fields.json#/fields"
            },
            {
              "component": "boolean",
              "name": "showLink",
              "label": "Replace consent text with weblink(s)",
              "valueType": "boolean"
            }
          ]
        },
        {
          "...": "../../models/form-common/_help-container.json"
        }
      ]
    }
  ]
}